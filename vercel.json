{
  "version": 2,
  "builds": [
    {
      "src": "working-backwards-assistant/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "working-backwards-assistant/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "working-backwards-assistant/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "working-backwards-assistant/$1"
    }
  ]
} 